
include $(MAKE_INCLUDE_GEN)

.PHONY: lib clean

#*****************************************************************************#
#                               VARIABLES	                                  #
#*****************************************************************************#

#*****************************************************************************#
#                        RULES TO GENERATE TARGETS                            #
#*****************************************************************************#

# Define the Rules to tar the libs

lib:
	$(AR) rvs lib_playback.a  *.o
	$(FROMELF) -g -D lib_playback.a
	$(MOVE) -f lib_playback.a $(ROOTDIR)/lib/application


#*****************************************************************************#
#                        RULES TO CLEAN TARGETS                               #
#*****************************************************************************#
clean:
	-rm *.o


-include $(DEPS)
